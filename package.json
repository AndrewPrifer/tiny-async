{
  "name": "tiny-async",
  "description": "Tiny, flexible and ergonomic async data fetching & RPC library for React with memoization support",
  "keywords": [
    "react",
    "async",
    "hook",
    "request",
    "query",
    "data",
    "cache",
    "fetch",
    "memoize",
    "promise",
    "swr"
  ],
  "license": "MIT",
  "author": "Andrew Prifer (https://github.com/AndrewPrifer)",
  "repository": "AndrewPrifer/tiny-async",
  "bugs": "https://github.com/AndrewPrifer/tiny-async/issues",
  "homepage": "https://github.com/AndrewPrifer/tiny-async",
  "version": "0.1.6",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  },
  "peerDependencies": {
    "react": "*"
  }
}
